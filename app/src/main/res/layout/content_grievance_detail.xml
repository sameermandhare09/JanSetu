                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="11dp"
                android:text="Sent From "

                />

            <TextView
                android:id="@+id/txtView_address"
                android:layout_width="wrap_content"
                android:textStyle="bold"
                android:textSize="11dp"
                android:paddingLeft="2dp"
                android:layout_gravity="right"
                android:layout_height="wrap_content" />

        </LinearLayout>


        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:padding="5dp"
            android:id="@+id/lnr_acknowledge"
          >

            <TextView

                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Acknowledge by "
                android:textSize="11dp"
                />

            <TextView
                android:id="@+id/txtView_acknowledgeby"
                android:layout_width="wrap_content"
                android:textStyle="bold"
                android:paddingLeft="2dp"
                android:layout_gravity="right"
                android:textSize="11dp"
                android:layout_height="wrap_content" />
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text=" On "
                android:textSize="11dp"
                />
            <TextView
                android:id="@+id/txtView_acknowledgedate"
                android:layout_width="wrap_content"
                android:textStyle="bold"
                android:paddingLeft="2dp"
                android:layout_gravity="right"
                android:textSize="11dp"
                android:layout_height="wrap_content" />



        </LinearLayout>


